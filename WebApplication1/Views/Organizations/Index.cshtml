@model IEnumerable<OrganizationsWaterSupplyL4.Models.Organization>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.OrgName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OwnershipType)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Adress)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DirectorFullname)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DirectorPhone)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ResponsibleFullname)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ResponsiblePhone)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.OrgName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OwnershipType)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Adress)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DirectorFullname)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DirectorPhone)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ResponsibleFullname)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ResponsiblePhone)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.OrganizationId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.OrganizationId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.OrganizationId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
